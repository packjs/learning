{
  "name": "electron-vue3-ts",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "electron:dev": "wait-on tcp:3000 && cross-env NODE_ENV=development electron ./",
    "electron:build": "electron-builder"
  },
  "dependencies": {
    "ant-design-vue": "^3.2.2",
    "vue": "^3.2.25"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^2.3.1",
    "cross-env": "^7.0.3",
    "electron": "^18.1.0",
    "electron-builder": "^23.0.3",
    "typescript": "^4.5.4",
    "unplugin-vue-components": "^0.19.3",
    "vite": "^2.9.5",
    "vue-tsc": "^0.34.7",
    "wait-on": "^6.0.1"
  },
  "build": {
    "productName": "zhifou",
    "appId": "co.zhifou",
    "copyright": "zhifou80@163.com",
    "directories": {
      "output": "output"
    },
    "files": [
      "dist/**/*",
      "electron/**/*"
    ],
    "nsis": {
      "oneClick": false,
      "allowElevation": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true
    },
    "linux": {}
  }
}
